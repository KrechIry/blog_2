body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    background-color: #eaeaea;
}

header {
    background-color: #833d00;
    color: white;
    padding: 20px 0;
}

nav ul {
    list-style-type: none;
    padding: 0;
    text-align: center;
}

nav ul li {
    display: inline;
    margin: 0 15px;
}

nav ul li a {
    color: white;
    text-decoration: none;
}

.container {
    text-align: center;
}

h1 {
    font-size: 2.5em;
    margin: 20px 0;
}

.learn-more {
    background-color: rgb(144, 50, 7);
    color: white;
    padding: 10px 20px;
    text-decoration: none;
    border-radius: 5px;
}

.section {
    padding: 50px 0;
    background-color: #fff;
    margin: 20px 0;
    text-align: center;
}

.content h2 {
    font-size: 2em;
    margin: 10px 0;
}



footer {
    background-color: #333;
    color: white;
    text-align: center;
    padding: 10px 0;
}